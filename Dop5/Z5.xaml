<Window x:Class="Dop5.Z5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dop5"
        mc:Ignorable="d"
        Title="Z5" Height="450" Width="800">
    <Grid Background="Aquamarine">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2.5*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <Image Grid.Column="0" >
            <Image.Source>
                <FormatConvertedBitmap Source="image/2.png" DestinationFormat="Gray32Float" ></FormatConvertedBitmap>
            </Image.Source>
        </Image>
        <StackPanel Grid.Column="1">
            <Image Source="image/2.png" MaxHeight="100" MaxWidth="100" Height="100" Width="100"/>
            <Image Source="image/1.jpg" MaxHeight="100" MaxWidth="100" Height="100" Width="100"/>
            <Image Source="image/3.jpg" MaxHeight="100" MaxWidth="100" Height="100" Width="100"/>
            <Image Source="image/4.jpg" MaxHeight="100" MaxWidth="100" Height="100" Width="100"/>
        </StackPanel>
        <Button Content="Zadanie6" HorizontalAlignment="Left" Margin="486,362,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
